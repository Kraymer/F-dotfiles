# Local/private config goes in the include
[include]
    path = ~/.config/git/credentials

# From there, sections are in alphabetic order

[alias]
    st = status
[color]
    diff = auto
    status = auto
    branch = auto
	ui = true
[help]
    autocorrect = 1
[init]
    templateDir = ~/.config/git/templates
[core]
    editor = nano
    excludesfile = ~/.gitignore
[color]
    diff = auto
    status = auto
    branch = auto
	ui = true
[pull]
	rebase = false
